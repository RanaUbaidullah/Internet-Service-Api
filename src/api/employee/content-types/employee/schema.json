{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "singularName": "employee",
    "pluralName": "employees",
    "displayName": "employee",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "userId": {
      "type": "uid"
    },
    "cnic": {
      "type": "biginteger",
      "required": true,
      "max": "9999999999999",
      "min": "999999999999"
    },
    "userName": {
      "type": "string"
    },
    "password": {
      "type": "password"
    },
    "employeeEmail": {
      "type": "email"
    },
    "employeeImg": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "phone": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "areaId": {
      "type": "integer"
    },
    "addedDate": {
      "type": "date"
    },
    "status": {
      "type": "boolean"
    },
    "contact_flag": {
      "type": "string"
    },
    "last_login": {
      "type": "date"
    },
    "employeeRole": {
      "type": "enumeration",
      "enum": [
        "Admin",
        "User",
        "Recovery Boy"
      ]
    }
  }
}
